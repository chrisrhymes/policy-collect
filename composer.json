{
    "name": "chrisrhymes/policy-collect",
    "description": "A Laravel Collection method to check a collection of models against a policy",
    "type": "project",
    "license": "Mit",
    "authors": [
        {
            "name": "Chris Rhymes",
            "email": "csrhymes@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ChrisRhymes\\PolicyCollect\\PolicyCollectServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.4",
        "laravel/legacy-factories": "^1.3"
    },
    "autoload": {
        "psr-4": {"ChrisRhymes\\PolicyCollect\\": "src/"}
